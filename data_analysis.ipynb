    "import pandas as pd\n",
    "from veryprettytable import VeryPrettyTable\n",
    "\n",
    "def analyze_file(filename):\n",
    "    sheet_names=['Sep2019','Oct2019','Nov2019']\n",
    "    for sheet in sheet_names:\n",
    "        print(sheet+\":\")\n",
    "        #data frame for certain sheet\n",
    "        if(sheet=='Oct2019'):\n",
    "            df= pd.read_excel(filename,sheet,4)\n",
    "        else:\n",
    "            df= pd.read_excel(filename,sheet,3)\n",
    "        #calculating total installs and cost\n",
    "        if(sheet=='Nov2019'):\n",
    "            total_installs= df.query(\"Date == 'Totals'\")[\"Installs\"].sum()\n",
    "        else:\n",
    "            total_installs= df.query(\"Date == 'Total'\")[\"Installs\"].sum()\n",
    "        df[\"Cost\"]=df[\"Cost\"].str[:-1].astype(float) #cutting out $ sign from value\n",
    "        total_cost= df['Cost'].sum()\n",
    "        #orginaze the data in a table\n",
    "        total_data = VeryPrettyTable()\n",
    "        total_data.field_names = [\"Total installs\", \"Total cost\"]\n",
    "        total_data.add_row([total_installs, total_cost])\n",
    "        print (total_data)\n",
    "        \n",
    "        #data frame for total installs and cost per date\n",
    "        data_by_date= VeryPrettyTable()\n",
    "        data_by_date.field_names = [\"Date\",\"Total installs\", \"Total cost\"]\n",
    "        df_date= df.groupby(['Date'])['Installs','Cost'].sum()\n",
    "        \n",
    "        for row in df_date.itertuples():\n",
    "            data_by_date.add_row([row[0],row[1],row[2]])\n",
    "        numOfRows=df_date['Installs'].count()\n",
    "        data_by_date.del_row(numOfRows-1)\n",
    "        print(data_by_date)\n",
    "        \n",
    "        #data frame for total installs and cost per app+platform\n",
    "        data_by_app_platf= VeryPrettyTable()\n",
    "        data_by_app_platf.field_names = [\"App\",\"Platform\",\"Total installs\", \"Total cost\"]\n",
    "        df_platform= df.groupby(['App','Campaign'])['Installs','Cost'].sum().reset_index()\n",
    "        for row in df_platform.itertuples():\n",
    "            platform= row[2].split()[0].strip()\n",
    "            data_by_app_platf.add_row([row[1],platform,row[3],row[4]])\n",
    "        print(data_by_app_platf)\n",
    "        \n",
    "        #data frame for total installs and cost per country\n",
    "        data_by_country= VeryPrettyTable()\n",
    "        data_by_country.field_names = [\"Country\",\"Total installs\", \"Total cost\"]\n",
    "        df_country= df.groupby(['Country'])['Installs','Cost'].sum()\n",
    "        for row in df_country.itertuples():\n",
    "            data_by_country.add_row([row[0],row[1],row[2]])\n",
    "        print(data_by_country)\n",
    "        pass\n",
    "\n",
    "analyze_file(FILENAME)"
   ]
